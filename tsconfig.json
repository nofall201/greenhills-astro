{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "esnext",
    "target": "esnext",
    "strict": true,
    "noEmit": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "types": ["astro/client"]
  },
  // "include": [".astro/types.d.ts", "**/*","src"],
  "include": ["src"],
  "exclude": ["dist"]
}


